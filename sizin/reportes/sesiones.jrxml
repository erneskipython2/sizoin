	<queryString>
		<![CDATA[SELECT
     usuario_sesion.`id_usuario_sesion` AS usuario_sesion_id_usuario_sesion,
     usuario_sesion.`fecha_ingreso` AS usuario_sesion_fecha_ingreso,
     usuario.`usuario` AS usuario_usuario,
     usuario_tipo.`usuario_tipo` AS usuario_tipo_usuario_tipo,
     personal.`cedula` AS personal_cedula
FROM
     `usuario` usuario INNER JOIN `usuario_sesion` usuario_sesion ON usuario.`id_usuario` = usuario_sesion.`id_usuario`
     INNER JOIN `personal` personal ON usuario.`id_personal` = personal.`id_personal`
     INNER JOIN `usuario_tipo` usuario_tipo ON usuario.`id_usuario_tipo` = usuario_tipo.`id_usuario_tipo`
WHERE
     personal.`id_sede` = $P{Sede}]]>
